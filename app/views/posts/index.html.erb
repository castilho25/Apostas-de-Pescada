<h1> Listing Posts </h1>

<table>
	<tr>
		<th>Title</th>
		<th>Description</th>
		<th></th>
	</tr>
	
	<% @posts.each do |post| %>
		<tr>
			<td><%= post.title %> </td>
			<td>  <%= post.description %></td>
			<td> <%= link_to 'Show Post', post_path(post) %></td>
			<td> <%= link_to 'Edit Post', edit_post_path(post) %> </td>
		</tr>


	<% end %>
	
	
</table>
	<%= link_to 'New Post', new_post_path %>